/ {
    combos {
        compatible = "zmk,combos";

        combo_alt_tab_switch_thumb {
            timeout-ms = <50>;
            key-positions = <POS_LH_T1 POS_LH_T2>;
            bindings = <&mod_tab_chord_v1_TKZ LALT LAYER_AltTab>;
        };
        combo_alt_tab_switch_index {
            timeout-ms = <50>;
            key-positions = <POS_LH_C1R2 POS_LH_C2R2>;
            bindings = <&mod_tab_chord_v1_TKZ LALT LAYER_AltTab>;
        };

        // Calls macro tab_switcher_ctrl_combo on Left T4/T5 - TailorKey
        combo_ctrl_tab_switch_thumb {
            timeout-ms = <50>;
            key-positions = <POS_LH_T2 POS_LH_T3>;
            bindings = <&mod_tab_chord_v1_TKZ LCTRL LAYER_CtrlTab>;
        };

        // Calls macro tab_switcher_ctrl_combo on Left T4/T5 - TailorKey
        combo_ctrl_tab_switch_index {
            timeout-ms = <50>;
            key-positions = <POS_LH_C1R1 POS_LH_C2R1>;
            bindings = <&mod_tab_chord_v1_TKZ LCTRL LAYER_CtrlTab>;
        };

